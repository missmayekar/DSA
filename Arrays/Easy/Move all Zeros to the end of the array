  public static void main(String[] args) {
        int[] array = {1, 2, 0, 1, 0, 4, 0};
        int i = 0;
        for(int j = 0; j < array.length; j++){
            if (array[j] != 0){
                int temp = array[i];
                array[i] = array[j];
                array[j] = temp;
                i++;
            }
        }
        System.out.println(Arrays.toString(array));
    }
